.page-special-offers
    .section-title.text-center
      .container-fluid
        h2.block-title SPECIAL OFFERS
        p.sub-title 
          span.line
          span.text Exclusive Packages
        p.sub-child-title FOR REMARKABLE HOLIDAYS
    .section-form-control
        .container-fluid
            form(action="")
                .form-group
                    .col-sm-6.col-sm-offset-2
                        .row
                            .col-xs-2
                                label(class="control-label") Region
                            .col-xs-10.col-region
                                .region-dropdown
                                    button(class="region-dropdown-toggle") Please Select
                                    .region-dropdown-menu
                                        a(href="#", class="region-dropdown-item active") Please Select
                                        a(href="#", class="region-dropdown-item") Americas
                                        a(href="#", class="region-dropdown-item") Asia Pacific
                                        a(href="#", class="region-dropdown-item") Europe
                                        a(href="#", class="region-dropdown-item") Middle East & Africa
    .section-new-special-offers
