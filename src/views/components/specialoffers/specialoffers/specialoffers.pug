                        a(data-filter=".early") EARLY BOOKER
                    li
                        a(data-filter=".suitely") SUITELY SENSATIONAL
                    li
                        a(data-filter=".wellness") WELLNESS & SPA
                    li
                        a(data-filter=".romance") ROMANCE
                    li
                        a(data-filter=".family") FAMILY & KIDS
                    li
                        a(data-filter=".golf") GOLF
