#offers.home-gallery
    .section-title.text-center
        .container-fluid
            h2.block-title.wow.fadeInUp #KEMPINSKI GALLERY
            p.sub-title.wow.fadeInUp(data-wow-delay="0.1s") 
                span.text Our guests always travel the world in style. Mention @Kempinski on Instagram for a chance to be featured!
    .container-fluid.container
        .gallery-slide
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#anasalali")
                    .bg-dark @anasalali
                    img(src="../../../../assets/images/anasanali.jpg", alt="anasanali")
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#bernabetv")
                    .bg-dark @theresabernabetv
                    img(src="../../../../assets/images/bernabetv.jpg", alt="bernabetv")
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#tourismsa")
                    .bg-dark @seychellestourismsa
                    img(src="../../../../assets/images/tourismsa.jpg", alt="seychellestourismsa")
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#grotesque")
                    .bg-dark @anny_grotesque
                    img(src="../../../../assets/images/grotesque.jpg", alt="anny_grotesque")
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#dubai")
                    .bg-dark @brasileiros_em_dubai
                    img(src="../../../../assets/images/dubai.jpg", alt="brasileiros_em_dubai")
            .item
                a(href="", class="gallery-item", data-fancybox="gallery", data-src="#kandaclub")
                    .bg-dark @kandaclub
                    img(src="../../../../assets/images/kandaclub.jpg", alt="kandaclub")
    .container-fluid.container
        .gallery-footer
            a(href="", class="btn-further-info") View gallery
            a(href="", class="btn-further-info") Add a photo
            .gallery-copy Powered by Olapic
#anasalali.gallery-slide2
    .container-fluid
        .row
            .col-md-7.col-photo
                img(src="../../../../assets/images/anasanali.jpg", alt="anasanali")
                a(href="", class="report-image ", title="Report photo?") 
            .col-md-5
                .user
                    a(href="")
                        img(src="../../../../assets/images/avatar.png", alt="avatar")
                        .author-name
                            p.username k2359k
                            p.realname k2359k
                p.photo-caption Palm trees and cool breeze  ⁣ ⁣ ⁣ ⁣ ⁣ ⁣ ⁣ ⁣ #bitsofmyworld_ #deadsea #jordan #amman #lovejo #حب_الأردن _أردننا #summer #beach #beachlife #palmtree #quotes #sayings #pool #vacation #relax #travel #traveller #follow #followtrain #like #likes #beauty #nature #photography #landscape #photooftheday #monday #mood #instagood
                .photo-sharing
                    p Share
                    ul
                        li
                            a(href=""): i.fa.fa.fa-facebook
                        li
                            a(href=""): i.fa.fa.fa-twitter
                        li
                            a(href=""): i.fa.fa.fa-pinterest-p
                h3 Book This Hotel Now
                .hotel-info
                    a(href="")
                        img(src="../../../../assets/images/anasanali.jpg", alt="anasanali")
                        p  Hotel california hotel california
                a(href="")
                    p.photo-copy Powered by Olapic
